/*
  input: 145
  output: 1 4 5
  */
#include <stdio.h>

int main()
{
   int n=0,a[100],i;
   long x;
   scanf("%ld",&x);
   while(x!=0)
   {
       
       a[n]=x%10;
       n++;
       x=x/10;
   }
   for(i=n-1;i>=0;i--)
   {
       printf("%d ",a[i]);
   }

    return 0;
}
