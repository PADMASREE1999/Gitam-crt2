/* dhyanahita  4  --> dhyanaatih (last for characters reversed) */
#include <stdio.h>
#include<string.h>

int main()
{
    int i,l,j,n;
    char x[100];
    scanf("%s",x);
    scanf("%d",&n);
    l=strlen(x);
    for(i=0;i<l-n;i++)    {
        printf("%c",x[i]);
    }
    
        for(i=l-1;i>=l-n;i--)
        {
            printf("%c",x[i]);
        }
    
    return 0;
}
