/* print the max and min values in an array
input:1 23 32 55 32
output:<1,55>
*/
#include <stdio.h>

int main()
{
  
      int a[100],n,i,max,min;
      scanf("%d",&n);
      for(i=0;i<n;i++)
      {
          scanf("%d",&a[i]);
          
      }
      min=max=a[0];
      for(i=0;i<n;i++)
      {
          if(max<a[i])
          {
              max=a[i];
              
          }
          if(min>a[i])
          {
              min=a[i];
          }
      }printf("<%d,%d>",min,max);
      

    return 0;
}

